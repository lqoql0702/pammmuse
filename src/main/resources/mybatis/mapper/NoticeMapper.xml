<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pammmuse.pammmuse.repository.NoticeMapper">

    <!-- 게시판 목록 -->
    <select id="getNotice" resultType="com.pammmuse.pammmuse.model.NoticeVo">

        select * from notice

    </select>

    <!-- 게시판 조회 -->
    <select id="getDetail" resultType="com.pammmuse.pammmuse.model.NoticeVo">

        select * from notice where id = #{id}

    </select>




</mapper>